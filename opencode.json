{
  "$schema": "https://opencode.ai/config.json",
  "plugin": [
    "oh-my-opencode",
    "opencode-openai-codex-auth",
    "cc-safety-net",
    "@tarquinen/opencode-dcp@latest"
  ],
  "disabled_providers": ["anthropic"],
  "permission": {
    "externalnetwork": "ask",
    "list:":"allow",
    "task":"allow",
    "todoread":"allow",
    "todowrite":"allow",
    "bash": {
      "*":"ask",
      "sudo*": "deny",
      "rm*":"ask",
      "rg*":"allow",
      "ls*":"allow",
      "git*":"ask",
      "git status *":"allow",
      "git diff*":"allow",
      "git show*":"allow",
      "gh pr view*":"allow",
      "gh pr diff*":"allow",
      "git log*":"allow",
    },
    "edit": {
      "*":"ask",
      "C:/GIT/*":"allow"
    },
    "read": {
      "*": "allow",
      "*.env": "deny",
      "*.env.*": "deny",
      "*.env.example": "allow"
    }
  },
  "mcp": {
    "linear": {
      "type": "local",
      "command": ["npx", "-y", "mcp-remote", "https://mcp.linear.app/mcp"],
      "enabled": true,
    }
  },
  "provider": {
    "openai": {
      "options": {
        "reasoningEffort": "medium",
        "reasoningSummary": "auto",
        "textVerbosity": "medium",
        "include": ["reasoning.encrypted_content"],
        "store": false
      },
      "models": {
        "gpt-5.2-none": {
          "name": "GPT 5.2 None (OAuth)",
          "limit": { "context": 272000, "output": 128000 },
          "modalities": { "input": ["text", "image"], "output": ["text"] },
          "options": {
            "reasoningEffort": "none",
            "reasoningSummary": "auto",
            "textVerbosity": "medium",
            "include": ["reasoning.encrypted_content"],
            "store": false
